{% extends 'base.html.twig' %}
{% block body %}
        <table class="table table-dark" id="table">
            <thead>
            <tr>
                <th scope="col">ContactName</th>
                <th scope="col">ContactAddress</th>
                <th scope="col">Number_1</th>
                <th scope="col">Number_2</th>
                <th scope="col">Number_3</th>
                <th scope="col">Action</th>
            </tr>
            </thead>
            <tbody>
            {{ qaq }}
{% for contact in contactList %}
            <tr>
                <th scope="row">{{ contact.ContactName }}</th>
                <td>{{ contact.ContactAddress }}</td>
                <td>{{ contact.Number_1 }}</td>
                <td>{{ contact.Number_2 }}</td>
                <td>{{ contact.Number_3 }}</td>
                <td><a href="/Contact/{{ contact.id }}/edit">Edit</a> <a href="/Contact/{{ contact.id }}/delete">Delete</a> </td>
            </tr>
{% endfor %}

            </tbody>
        </table>

{% endblock %}
